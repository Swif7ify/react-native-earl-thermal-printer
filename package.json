{
	"name": "react-native-earl-thermal-printer",
	"version": "1.0.0",
	"description": "A modern, high-performance thermal printer library for React Native. Built with the New Architecture (TurboModules) for synchronous communication, zero legacy bridge overhead, and Android 12+ Bluetooth compliance.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"source": "src/index.ts",
	"scripts": {
		"pods": "cd example && pod-install --quiet",
		"example": "yarn --cwd example",
		"bootstrap": "yarn example && yarn && yarn pods",
		"build": "rimraf ./dist && tsc",
		"prepare": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Swif7ify/react-native-earl-thermal-printer.git"
	},
	"files": [
		"dist",
		"src",
		"android",
		"ios",
		"react-native-earl-thermal-printer.podspec"
	],
	"codegenConfig": {
		"name": "RNThermalReceiptPrinterSpec",
		"type": "modules",
		"jsSrcsDir": "src",
		"android": {
			"javaPackageName": "com.pinmi.react.printer"
		}
	},
	"keywords": [
		"react-native",
		"thermal",
		"printer",
		"bluetooth",
		"net",
		"usb",
		"turbomodule",
		"new-architecture",
		"android",
		"ios"
	],
	"dependencies": {
		"buffer": "^5.6.0",
		"iconv-lite": "^0.6.2",
		"string_decoder": "^1.3.0"
	},
	"peerDependencies": {
		"react": ">=18.0.0",
		"react-native": ">=0.73.0"
	},
	"devDependencies": {
		"@types/react": "^18.2.0",
		"react": "^18.2.0",
		"react-native": "^0.73.0",
		"rimraf": "^5.0.0",
		"typescript": "^5.3.0"
	},
	"author": "Ordovez, Earl Romeo",
	"homepage": "https://github.com/Swif7ify/react-native-earl-thermal-printer#readme",
	"license": "ISC"
}
